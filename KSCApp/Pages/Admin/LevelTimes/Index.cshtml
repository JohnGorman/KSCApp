@page
@model KSCApp.Pages.Admin.LevelTimes.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h2>Level Playing Times</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LevelTime[0].Level)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LevelTime[0].StartTime)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.LevelTime) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Level)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartTime)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.LevelTimeId">Edit</a> |
                @*<a asp-page="./Details" asp-route-id="@item.LevelTimeId">Details</a> |*@
                <a asp-page="./Delete" asp-route-id="@item.LevelTimeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
